{
  "evil": true,
  "regexdash": true,
  "browser": true,
  "wsh": true,
  "trailing": true,
  "sub": true,
  "undef": true,
  "unused": true,
  "predef": [
    /* Testing globals */
    "xit",
    "it",
    "xdescribe",
    "describe",
    "expect",
